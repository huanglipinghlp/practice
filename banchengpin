			for(int p=0;p<number;p++)
			{
		        szgs=(int)(Math.random()*2+3);//生成3-4个数字
			    fhgs=szgs-1;//字符个数比数字少1
			    float rs[]=new float[20];
			    float c;String s;
			    char op[]=new char[10];//储存字符
			    for(y=0;y<szgs;y++)
			    {
			        sz[y]=(int)(Math.random()*max+1);//随机生成数
