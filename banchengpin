import java.text.DecimalFormat;
import java.util.Scanner;
public class test {
	static int simplify(int x,int y){/*求两数最大公约数，用于分数化简*/
    	int t,xx,yy,k=1;
    	if(x>y)
    	{
    		t=x;x=y;y=t;
    	}
    	for(int i=x;i>=2;i--)
    	{
    		  xx=x%i;yy=y%i;
    		  if(xx==0&&yy==0)
    		  {
    			  k=i;
    			  break;
    		  }	 
    		}
    	return k;
    }
	
